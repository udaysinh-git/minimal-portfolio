# GitHub Personal Access Token (for GitHub data)
GITHUB_TOKEN=your_github_personal_access_token

# Google Sheets Configuration (for blog views, Spotify cache, Discord activity cache)
GOOGLE_SHEET_ID=your_google_sheet_id
GOOGLE_SHEETS_CLIENT_EMAIL=your_service_account_email
GOOGLE_SHEETS_PRIVATE_KEY=your_private_key

# Discord Activity (for showing current activity status)
ACTIVITIES_USER_ID=your_discord_user_id

# Spotify Configuration (optional)
SPOTIFY_CLIENT_ID=your_spotify_client_id
SPOTIFY_CLIENT_SECRET=your_spotify_client_secret
SPOTIFY_REFRESH_TOKEN=your_spotify_refresh_token

# Contact Form (optional)
TURNSTILE_SECRET_KEY=your_turnstile_secret_key
DISCORD_WEBHOOK_URL=your_discord_webhook_url

